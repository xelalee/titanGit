<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
 <managed-bean>
  <managed-bean-name>applicationBean</managed-bean-name>
  <managed-bean-class>com.titan.mytitan.app.ApplicationBean</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>AdministratorAction</managed-bean-name>
  <managed-bean-class>com.titan.admin.account.action.AdministratorAction</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>administratorBean</managed-bean-name>
  <managed-bean-class>com.titan.admin.account.bean.AdministratorBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean> 
 <managed-bean>
  <managed-bean-name>GroupAction</managed-bean-name>
  <managed-bean-class>com.titan.admin.account.action.GroupAction</managed-bean-class>
