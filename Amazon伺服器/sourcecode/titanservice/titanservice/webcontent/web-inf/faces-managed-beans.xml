 <managed-bean>
  <managed-bean-name>myServiceAction</managed-bean-name>
  <managed-bean-class>com.titan.mytitan.service.action.MyServiceAction</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>localeBean</managed-bean-name>
  <managed-bean-class>com.titan.mytitan.login.bean.LocaleBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
</faces-config>
