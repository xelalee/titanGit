<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
 <navigation-rule>
	  <from-view-id>*</from-view-id>
	  <navigation-case>
	   <from-outcome>adminLogin</from-outcome>
	   <to-view-id>/jsp/admin/login/login.jsp</to-view-id>
	  </navigation-case>  
	  <navigation-case>
	   <from-outcome>home</from-outcome>
	   <to-view-id>/jsp/login/welcome.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>login</from-outcome>
	   <to-view-id>/jsp/login/login.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>myaccount_update</from-outcome>
	   <to-view-id>/jsp/myaccount/myaccount_update.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>myproduct_list</from-outcome>
	   <to-view-id>/jsp/myproduct/myproduct_list.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>myservice_main</from-outcome>
	   <to-view-id>/jsp/myservice/myservice_main.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>logout</from-outcome>
	   <to-view-id>/jsp/login/logout.jsp</to-view-id>
	  </navigation-case>
 </navigation-rule>
 <navigation-rule>
	  <from-view-id>/jsp/login/login.jsp</from-view-id>
	  <navigation-case>
	   <from-outcome>success</from-outcome>
	   <to-view-id>/jsp/login/welcome.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>retry</from-outcome>
	   <to-view-id>/jsp/login/login.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>confirm</from-outcome>
	   <to-view-id>/jsp/login/login_confirm.jsp</to-view-id>
	  </navigation-case>
 </navigation-rule>
 <navigation-rule>
	  <from-view-id>/jsp/admin/login/login.jsp</from-view-id>
	  <navigation-case>
	   <from-outcome>success</from-outcome>
	   <to-view-id>/jsp/admin/common/welcome.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>retry</from-outcome>
	   <to-view-id>/jsp/admin/login/login.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>confirm</from-outcome>
	   <to-view-id>/jsp/login/login_confirm.jsp</to-view-id>
	  </navigation-case>
 </navigation-rule>
 <navigation-rule>
	  <from-view-id>/jsp/login/login_confirm.jsp</from-view-id>
	  <navigation-case>
	   <from-outcome>success</from-outcome>
	   <to-view-id>/jsp/login/welcome.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>retry</from-outcome>
	   <to-view-id>/jsp/login/login.jsp</to-view-id>
	  </navigation-case>
 </navigation-rule>
 <navigation-rule>
	  <from-view-id>/jsp/myaccount/myaccount_create.jsp</from-view-id>
	  <navigation-case>
	   <from-outcome>success</from-outcome>
	   <to-view-id>/jsp/myaccount/myaccount_create_confirm.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>failure</from-outcome>
	   <to-view-id>/jsp/myaccount/myaccount_create.jsp</to-view-id>
	  </navigation-case>
 </navigation-rule>
 <navigation-rule>
	  <from-view-id>/jsp/myaccount/myaccount_update.jsp</from-view-id>
	  <navigation-case>
	   <from-outcome>success</from-outcome>
	   <to-view-id>/jsp/myaccount/myaccount_update_confirm.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>failure</from-outcome>
	   <to-view-id>/jsp/myaccount/myaccount_update.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>change_password</from-outcome>
	   <to-view-id>/jsp/myaccount/change_password.jsp</to-view-id>
	  </navigation-case>
 </navigation-rule>
 <navigation-rule>
	  <from-view-id>/jsp/myaccount/change_password.jsp</from-view-id>
	  <navigation-case>
	   <from-outcome>success</from-outcome>
	   <to-view-id>/jsp/myaccount/change_password_confirm.jsp</to-view-id>
	  </navigation-case>
 </navigation-rule>
 <navigation-rule>
	  <from-view-id>/jsp/login/retrieve_password.jsp</from-view-id>
	  <navigation-case>
	   <from-outcome>retry</from-outcome>
	   <to-view-id>/jsp/login/retrieve_password.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>success</from-outcome>
	   <to-view-id>/jsp/login/retrieve_password.jsp</to-view-id>
	  </navigation-case>
 </navigation-rule>
 <navigation-rule>
	  <from-view-id>/jsp/myservice/myservice_main.jsp</from-view-id>
	  <navigation-case>
	   <from-outcome>myproduct_rename</from-outcome>
	   <to-view-id>/jsp/myproduct/myproduct_rename.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>myproduct_transfer</from-outcome>
	   <to-view-id>/jsp/myproduct/myproduct_transfer.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>myproduct_reinstall</from-outcome>
	   <to-view-id>/jsp/myproduct/myproduct_reinstall.jsp</to-view-id>
	  </navigation-case>
 </navigation-rule>
 <navigation-rule>
	  <from-view-id>/jsp/myproduct/myproduct_rename.jsp</from-view-id>
	  <navigation-case>
	   <from-outcome>success</from-outcome>
	   <to-view-id>/jsp/myproduct/myproduct_rename_confirm.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>cancel</from-outcome>
	   <to-view-id>/jsp/myservice/myservice_main.jsp</to-view-id>
	  </navigation-case>
 </navigation-rule>
 <navigation-rule>
	  <from-view-id>/jsp/myproduct/myproduct_transfer.jsp</from-view-id>
	  <navigation-case>
	   <from-outcome>success</from-outcome>
	   <to-view-id>/jsp/myproduct/myproduct_transfer_confirm.jsp</to-view-id>
	  </navigation-case>
 </navigation-rule>
 <navigation-rule>
	  <from-view-id>/jsp/myproduct/myproduct_reinstall.jsp</from-view-id>
	  <navigation-case>
	   <from-outcome>success</from-outcome>
	   <to-view-id>/jsp/myproduct/myproduct_reinstall_confirm.jsp</to-view-id>
	  </navigation-case>
 </navigation-rule>
 <navigation-rule>
	  <from-view-id>/jsp/admin/account/account_list.jsp</from-view-id>
	  <navigation-case>
	   <from-outcome>account_edit</from-outcome>
	   <to-view-id>/jsp/admin/account/account_edit.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>account_add</from-outcome>
	   <to-view-id>/jsp/admin/account/account_add.jsp</to-view-id>
	  </navigation-case>	  
 </navigation-rule>
 <navigation-rule>
	  <from-view-id>/jsp/admin/account/group_list.jsp</from-view-id>
	  <navigation-case>
	   <from-outcome>group_edit</from-outcome>
	   <to-view-id>/jsp/admin/account/group_edit.jsp</to-view-id>
	  </navigation-case>
	  <navigation-case>
	   <from-outcome>group_add</from-outcome>
	   <to-view-id>/jsp/admin/account/group_add.jsp</to-view-id>
	  </navigation-case>	  
 </navigation-rule>
 
</faces-config>
